<!--custom-tab-bar/index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item wx:for="{{ list }}" wx:key="index" bind:change="onChange">
    <image slot='icon' src="{{ item.image}}" style="width: 55rpx; height:55rpx" mode="aspectFill"></image>
    <image slot='icon-active' src="{{ item.selectedImage}}" style="width: 55rpx; height: 55rpx" mode="aspectFill"></image>
    {{item.text}}





  </van-tabbar-item>

</van-tabbar>